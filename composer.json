{
    "name": "shellrent/shellrent-api-sdk",
    "description": "PHP SDK for the Shellrent APIs",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shellrent\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shellrent\\Api\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^5.4",
        "nunomaduro/collision": "^6.1"
    }
}
